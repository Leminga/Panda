# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#POST	/login						controllers.UserLogin.save()
#GET	/							controllers.Application.index()
#GET	/LoginData					controllers.UserLogin.load()
POST	/login						controllers.SecurityController.login()
POST	/register					controllers.SecurityController.register()
#POST    /getUser                    controllers.VolunteerController.userRequest()
#PATCH  /saveUser                   controllers.VolunteerController.saveAll()

# Map static resources from the /public folder to the /assets URL path
GET		/assets/*file				controllers.Assets.at(path="/public", file)
GET		/							controllers.Assets.at(path="/public", file="index.html")
